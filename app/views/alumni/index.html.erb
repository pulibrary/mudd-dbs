<center><h1>Princeton Undergraduate Alumni Index, 1748-1920</h1></center>
<table border=1 align="center" class="datatable">
  <thead>
    <tr>
      <th>Box</th>
      <th>Last Name</th>
      <th>First Name</th>
      <th>Class</th>
      <th>Graduate</th>
      <th>Photos</th>
      <th>Oversize</th>
      <% if user_signed_in? %>
      <th></th>
      <th></th>
      <th></th>
      <% end %>
    </tr>
  </thead>

  <tbody>
    <% @alumni.each do |alumn| %>
      <tr>
        <td><%= alumn.box %></td>
        <td><%= alumn.lname %></td>
        <td><%= alumn.fname %></td>
        <td><%= alumn.year %></td>
        <td><%= alumn.graduate %></td>
        <td><%= alumn.photos %></td>
        <td><%= alumn.oversize %></td>
        <% if user_signed_in? %>
        <td><%= link_to 'Show', alumn %></td>
        <td><%= link_to 'Edit', edit_alumn_path(alumn) %></td>
        <td><%= link_to 'Destroy', alumn, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<% if user_signed_in? %>
<%= link_to 'New Alumn', new_alumn_path %>
<% end %>

<div style="width:100%; text-align:center;">
<br>
<%= link_to 'New Search', "http://rbscbeta.princeton.edu/databases/undergraduate-alumni-index-1748-1920" %>
<br>
<%= link_to 'Return to the Mudd Library website', "http://rbscbeta.princeton.edu/locations/seeley-g-mudd-manuscript-library" %>
</div>
